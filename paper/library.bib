@Article{Bruglieri2004,
  author   = {Maurizio Bruglieri and Francesco Maffioli and Matthias Ehrgott},
  title    = {Cardinality constrained minimum cut problems: complexity and algorithms},
  journal  = {Discrete Applied Mathematics},
