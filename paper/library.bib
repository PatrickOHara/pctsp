  address   = {Berlin, Heidelberg},
  publisher = {Springer Berlin Heidelberg},
  abstract  = {The degeneracy of an n-vertex graph G is the smallest number k such that every subgraph of G contains a vertex of degree at most k. We present an algorithm for enumerating all simple cycles of length p in an n-order k-degenerate graph running in time {\$}{\$}{\backslash}mathcal {\{}O{\}}(n^{\{}{\backslash}lfloor {\{}p/2{\}} {\backslash}rfloor {\}} k^{\{}{\backslash}lceil p/2 {\backslash}rceil {\}}){\$}{\$}. We then show that this algorithm is worst-case output size optimal by proving a {\$}{\$}{\backslash}varTheta (n^{\{}{\backslash}lfloor {\{}p/2{\}} {\backslash}rfloor {\}} k^{\{}{\backslash}lceil {\{}p{\}}/{\{}2{\}} {\backslash}rceil {\}}){\$}{\$}bound on the maximal number of simple p-length cycles in these graphs. Our results also apply to induced (chordless) cycles.},
