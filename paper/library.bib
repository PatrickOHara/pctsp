@InProceedings{Korkmaz1999,
  author    = {T. Korkmaz and M. Krunz},
  title     = {A randomized algorithm for finding a path subject to multiple QoS constraints},
  booktitle = {Seamless Interconnection for Universal Services. Global Telecommunications Conference. GLOBECOM'99. (Cat. No.99CH37042)},
  year      = {1999},
  volume    = {3},
  pages     = {1694-1698 vol.3},
  abstract  = {An important aspect of quality-of-service (QoS) provisioning in integrated networks is the ability to find a feasible route that satisfies end-to-end QoS requirements of a connection request while efficiently using network resources. In general, finding a path subject to multiple additive constraints (e.g., delay, delay-jitter) is an NP-complete problem. For this problem, we propose a randomized heuristic algorithm with polynomial-time complexity. Our algorithm first prunes all the links that cannot be on any feasible paths. It then uses a randomized heuristic search to find a feasible path, if one exists. The worst-case computational complexity of our algorithm is O(n/sup 2/), where n is the number of nodes. The storage complexity of the algorithm is O(n). Using extensive simulations, we show that our algorithm gives very high success rate in finding feasible paths.},
  doi       = {10.1109/GLOCOM.1999.832452},
  keywords  = {telecommunication network routing;computational complexity;quality of service;polynomials;directed graphs;computational complexity;multiple QoS constraints;quality-of-service;QoS provisioning;integrated networks;end-to-end QoS requirements;connection request;multiple additive constraints;delay-jitter;NP-complete problem;randomized heuristic algorithm;polynomial-time complexity;storage complexity;feasible route finding;feasible path finding;Bandwidth;Delay;Heuristic algorithms;Routing;Polynomials;Computational complexity;Costs;Approximation algorithms;Computer networks;Quality of service},
}

